//Write a function that doubles every second integer in a list, starting from the left.
//Example:
//doubleEveryOther([1,2,3,4]) => [ 1, 4, 3, 8 ]

function doubleEveryOther(a) {
  doubledArr = [];
  for (let i = 0; i <= a.length - 1; i += 1) {
    if (i % 2 !== 0) {
      doubledArr.push(a[i] * 2)
    } else {
    doubledArr.push(a[i])
    }
  }
  return doubledArr;
}
